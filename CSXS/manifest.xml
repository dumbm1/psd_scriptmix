<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest
        Version="4.0"
        ExtensionBundleId="com.wk.psd_scriptmix"
        ExtensionBundleVersion="1.2.1"
        ExtensionBundleName="psd_scriptmix"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Author>
        <![CDATA[Marat Shagiev]]>
    </Author>
    <Contact mailto="m_js@bk.ru">
        <![CDATA[m_js@bk.ru]]>
    </Contact>
    <Legal>
        <![CDATA[Legal stuff...]]>
    </Legal>
    <Abstract href="#">
        <![CDATA[Place your favorite jsx-scripts as a custom png-buttons]]>
    </Abstract>
    <ExtensionList>
        <Extension Id="com.wk.psd_scriptmix" Version="1.2.1"/>
    </ExtensionList>
    <ExecutionEnvironment>
        <HostList>
            <!--<Host Name="ILST" Version="17.0"/>-->
            <!--<Host Name="IDSN" Version="10.0"/>-->
            <Host Name="PHXS" Version="15.0"/>
            <Host Name="PHSP" Version="15.0"/>
        </HostList>
        <LocaleList>
            <Locale Code="All"/>
        </LocaleList>
        <RequiredRuntimeList>
            <RequiredRuntime Name="CSXS" Version="4.0"/>
        </RequiredRuntimeList>
    </ExecutionEnvironment>
    <DispatchInfoList>
        <Extension Id="com.wk.psd_scriptmix">
            <DispatchInfo>
                <Resources>
                    <MainPath>./index.html</MainPath>
                    <ScriptPath>./jsx/hostscript.jsx</ScriptPath>
                </Resources>
                <Lifecycle>
                    <AutoVisible>true</AutoVisible>
                </Lifecycle>
                <UI>
                    <Type>Panel</Type>
                    <Menu>"psd scriptmix 1-2-1"</Menu>
                    <Geometry>
                        <Size>
                            <Height>150</Height>
                            <Width>200</Width>
                        </Size>
                        <MinSize>
                            <Height>150</Height>
                            <Width>190</Width>
                        </MinSize>
                        <MaxSize>
                            <Height>150</Height>
                            <Width>240</Width>
                        </MaxSize>
                    </Geometry>
                    <Icons>
                        <Icon Type="Normal">./icons/iconNormal.png</Icon>
                        <Icon Type="RollOver">./icons/iconRollover.png</Icon>
                        <Icon Type="Disabled">./icons/iconDisabled.png</Icon>
                        <Icon Type="DarkNormal">./icons/iconDarkNormal.png</Icon>
                        <Icon Type="DarkRollOver">./icons/iconDarkRollover.png</Icon>
                    </Icons>
                </UI>
            </DispatchInfo>
        </Extension>
    </DispatchInfoList>
</ExtensionManifest>